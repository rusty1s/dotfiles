host i-* mi-*
  ProxyCommand sh -c "aws ssm start-session --target %h --region eu-central-1 --document-name AWS-StartSSHSession --parameters 'portNumber=%p'"
  User ubuntu
  IdentityFile ~/.ssh/kumo-init-key.pem
  ForwardAgent yes
Host *
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
  User matthias
